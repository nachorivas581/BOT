#! /bin/bash
#COMANDOS


key="$(cd /var/www/html/VPS-MOD ; ls | wc -l)"
status="$(systemctl is-active apache2)"
ID="$BOTID"
Usuario="$MYUSERNAME"
Nombre="$MYFIRSTNAME"
SSH="$(grep -c ".*" /etc/usuarios)"

(echo "
